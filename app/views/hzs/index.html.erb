<p id="notice"><%= notice %></p>

<h1>Hzs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Num</th>
      <th>Hz</th>
      <th>Client</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hzs.each do |hz| %>
      <tr>
        <td><%= hz.name %></td>
        <td><%= hz.num %></td>
        <td><%= hz.hz %></td>
        <td><%= hz.client %></td>
        <td><%= hz.date %></td>
        <td><%= link_to 'Show', hz %></td>
        <td><%= link_to 'Edit', edit_hz_path(hz) %></td>
        <td><%= link_to 'Destroy', hz, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hz', new_hz_path %>
